{
  "presets": [
    [ "@babel/preset-env", {
      "modules": false,
      "loose": true,
      "useBuiltIns": false,
      "targets": {
        "browsers": [ "last 2 versions", "not ie <= 8" ]
      },
      "exclude": [ "transform-typeof-symbol", "transform-function-name" ]
    } ]
  ],
  "plugins": [ "@babel/plugin-transform-runtime" ]
}
